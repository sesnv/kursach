<Window x:Class="Login.Model.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Login.Model"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DGridStudents" AutoGenerateColumns="False" IsReadOnly="True">
            <DataGrid.Columns>
                <DataGridTextColumn Header ="Фамилия" Binding="{Binding familiya}" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header ="Имя" Binding="{Binding imya}" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header ="Номер группы" Binding="{Binding Num_gr}" Width="150"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
